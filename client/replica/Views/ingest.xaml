                            <ComboBoxItem Content="{Binding Mode=OneWay, Converter={StaticResource g}, ConverterParameter=l, Path=cCommon.sNotApplicable, Source={StaticResource g}}" />
                            <ComboBoxItem>
                                <ComboBoxItem.Content>
                                    <Grid>
                                        <itk:NumericUpDown Minimum="1" Maximum="99" Name="_ui_nudAge" ValueChanged="_ui_nud_ValueChanged" />
                                    </Grid>
                                </ComboBoxItem.Content>
                            </ComboBoxItem>
                        </ComboBox>
                        <TextBlock Grid.Row="1" Grid.Column="0" Name="_ui_txtAction" Text="{Binding Path=cReplica.sNoticeIngest7, Source={StaticResource g}}" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,10,10,10" />
