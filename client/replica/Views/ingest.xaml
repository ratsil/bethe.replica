							<itk:ContextMenu Name="_ui_cmClipArtists" Opened="_ui_cmClipArtists_Opened">
								<itk:MenuItem Header="{Binding Path=cCommon.sRefresh, Source={StaticResource g}}" Click="_ui_cmClipArtists_Refresh" />
								<itk:Separator/>
